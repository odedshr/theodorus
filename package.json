{
  "name": "Theodorus",
  "description": "Theodorus is a back-end for online democratic communite",
  "version": "0.3.0",
  "main": "index.js",
  "private": true,
  "config": {
    "ipAddress": "127.0.0.1",
    "port": "5000",
    "dbConnectionString": "sqlite://build/theodorus.db",
    "guidLength": 3,
    "webAppFolder": "build/www",
    "storedFilesFolder": "build/uploads",
    "devUrl": "http://127.0.0.1:5000",
    "prodUrl": "https://theo-dorus.rhcloud.com/",
    "defaultMode": "prod",
    "defaultIncludeModules": "all",
    "defaultBuildWepApp": "auto"
  },
  "engines": {
    "node": ">= 0.10.0",
    "npm": ">= 1.0.0"
  },
  "scripts": {
    "start": "node index.js",
    "test": "node_modules/.bin/mocha",
    "supervise": "node_modules/.bin/supervisor index.js"
  },
  "dependencies": {
    "addressparser": "^1.0.1",
    "body-parser": "*",
    "chokidar": "^1.4.3",
    "cookie-parser": "^1.4.0",
    "deasync": "^0.1.5",
    "express": "^5.0.0-alpha.2",
    "formidable": "^1.0.17",
    "graceful-fs": "^4.1.3",
    "imagemagick": "*",
    "jwt-simple": "*",
    "less": "^2.6.1",
    "lodash": "^4.11.1",
    "marked": "^0.3.5",
    "md5": "*",
    "minimatch": "*",
    "moment": "^2.13.0",
    "mysql": "^2.10.0",
    "node-pre-gyp": "^0.6.19",
    "nodemailer": "^2.3.2",
    "nodemailer-direct-transport": "^3.3.2",
    "orm": "^2.1.30",
    "pug": "^2.0.0-alpha6",
    "sqlite3": "^3.1.0",
    "strip-json-comments": "*",
    "uglify-js": "^2.6.2",
    "underscore": "^1.8.3",
    "winston": "^1.1.1"
  },
  "devDependencies": {
    "mocha": "^2.3.3",
    "qunitjs": "*",
    "should": "^7.1.1",
    "supertest": "^1.1.0"
  }
}
